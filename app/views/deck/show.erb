<body class="main_bg">
  <article>
    <h2><%= @deck.name %></h2>
    <img class="deck_cover" src="<%= @deck.deck_img %>">
    <p><%= @deck.desc %></p>
    <form id="contact_form" action="/deck/<%= @deck.id %>" method="POST">
      <input type="submit" value="Start">
    </form>
	<%= erb :"deck/delete", :locals => {id: @deck.id} if @deck.creator_id == session[:user_id] %>
  </article>
</body>